@model List<RequestedRandomUserDataModel>;

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Random User Output</h1>
 

    <table class="table">
        <thead>
            <tr>
                <th scope="col">Title</th>
                <th scope="col">First Name</th>
                <th scope="col">Last Name</th>
                <th scope="col">Age</th>
                <th scope="col">Longtitude</th>
                <th scope="col">Latitude</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var randomUser in Model)
            {
                <tr>

                    <td>@randomUser.title</td>
                    <td>@randomUser.first</td>
                    <td>@randomUser.last</td>
                    <td>@randomUser.age</td>
                    <td>@randomUser.longitude</td>
                    <td>@randomUser.latitude</td>
                  
                </tr>
                }
            </tbody>
    </table>
</div>

